{
    "rules": {
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn",
        "react/prop-types": 0,
        "import/no-unresolved": 0
    },
    "plugins": [
        "react-hooks",
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "esnext",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended"
    ],
    
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}